{
  "plugins": [
    "prettier",
    "unicorn",
    "lodash"
  ],
  "extends": [
    "airbnb-base",
    "plugin:unicorn/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "prettier/prettier" : [
      "error", {
        "endOfLine": "auto",
        "singleQuote": true
      }
    ],
    "import/extensions": ["error",  {
      "js": "always",
      "mjs": "always"
    }]
  
  }
}